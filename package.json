{
  "name": "gatsby-starter-ts",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "setup": "node ./contentful/setup.js",
    "test": "vitest",
    "test:watch": "vitest watch",
    "gatsby-provision": "gatsby-provision-contentful --contentful-data-path='./contentful/export.json' --space-id=$CONTENTFUL_SPACE_ID --management-token=$CONTENTFUL_MANAGEMENT_TOKEN",
    "export-content": "contentful space export --space-id=$SPACE_ID --management-token=$MANAGEMENT_TOKEN --export-dir=scripts --content-file=data.json"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@contentful/rich-text-plain-text-renderer": "16.0.2",
    "gatsby": "5.3.3",
    "gatsby-plugin-image": "3.4.0",
    "gatsby-plugin-pnpm": "1.2.10",
    "gatsby-plugin-sharp": "5.4.0",
    "gatsby-source-contentful": "8.4.0",
    "gatsby-transformer-sharp": "5.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsconfig-paths-webpack-plugin": "4.0.0"
  },
  "devDependencies": {
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@types/node": "18.11.16",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "@vitejs/plugin-react": "3.0.0",
    "babel-preset-gatsby": "3.3.1",
    "c8": "7.12.0",
    "contentful-cli": "1.18.0",
    "contentful-import": "8.5.21",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-n": "15.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "gatsby-provision-contentful": "0.0.5",
    "husky": "8.0.2",
    "identity-obj-proxy": "3.0.0",
    "jsdom": "20.0.3",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "typescript": "4.9.4",
    "vite-tsconfig-paths": "4.0.3",
    "vitest": "0.26.2"
  }
}
